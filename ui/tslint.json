{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "quotemark": [true,"single"],
    "trailing-comma": [false],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": [false]
  }
}
